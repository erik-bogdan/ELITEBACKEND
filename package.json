{
  "name": "escvoting",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch --hot src/index.ts",
    "seed": "bun run src/database/seed.ts",
    "seed:teams": "bun run src/commands/seed-teams.ts",
    "seed:team-players": "bun run src/commands/seed-team-players.ts",
    "seed:matches": "bun run src/commands/seed-matches.ts",
    "seed:results": "bun run src/commands/seed-results.ts",
    "migrate": "bunx drizzle-kit migrate",
    "format": "bun run src/database/seed.ts format",
    "email": "email dev --dir src/emails",
    "db:reset": "bun run src/database/seed.ts format && bunx drizzle-kit migrate && bun run src/commands/seed.ts"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.1",
    "@elysiajs/static": "^1.3.0",
    "@elysiajs/swagger": "^1.3.0",
    "@react-email/components": "^0.5.1",
    "better-auth": "^1.2.8",
    "dayjs": "^1.11.13",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.43.1",
    "drizzle-typebox": "^0.3.2",
    "elysia": "latest",
    "pg": "^8.16.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@react-email/preview-server": "4.2.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "bun-types": "latest",
    "react-email": "^4.2.8"
  },
  "optionalDependencies": {
    "nodemailer": "^6.9.14",
    "@react-email/render": "^0.0.17"
  },
  "overrides": {
    "@sinclair/typebox": "0.34.27"
  },
  "module": "src/index.js",
  "packageManager": "pnpm@10.6.4+sha512.da3d715bfd22a9a105e6e8088cfc7826699332ded60c423b14ec613a185f1602206702ff0fe4c438cb15c979081ce4cb02568e364b15174503a63c7a8e2a5f6c"
}
